{
  "name": "beauty",
  "displayName": "Beautiful Code",
  "description": "Syntax Highlight for Beau Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "elg",
      "aliases": ["Beau", "elg"],
      "extensions": [".elg"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "elg",
      "scopeName": "source.elg",
      "path": "./syntaxes/elg.tmLanguage.json"
    }]
  }
}
